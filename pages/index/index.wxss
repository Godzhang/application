.item{
    position: relative;
    height: 80rpx;
    padding: 0 20rpx;
    line-height: 80rpx;
    border-bottom: 2rpx solid #ddd;
    font-size: 36rpx;
    color: #333;
}
.arrow-icon{
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
}
.arrow-icon image{
    width: 100%;
    height: 100%;
}




